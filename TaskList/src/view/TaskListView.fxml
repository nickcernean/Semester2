<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.TaskListViewController">
   <children>
      <TableView fx:id="tableView" prefHeight="478.0" prefWidth="600.0">
        <columns>
             <TableColumn fx:id="ownerColumn" prefWidth="206.39996337890625" text="Owner" />
            <TableColumn fx:id="descColumn" prefWidth="191.199951171875" text="Description" />
            <TableColumn fx:id="dateColumn" prefWidth="201.60003662109375" text="Date" />
        </columns>
      </TableView>
      <HBox prefHeight="216.0" prefWidth="600.0" spacing="30.0">
         <children>
            <Button mnemonicParsing="false" onAction="#openAddView" text="Add">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#openGetNextTaskView" text="Nex Task" />
         </children>
      </HBox>
   </children>
</VBox>
