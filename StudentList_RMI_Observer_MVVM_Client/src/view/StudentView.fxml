<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox prefHeight="550.0" prefWidth="443.0" userData="Student view" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.StudentViewController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="460.0" spacing="10.0">
         <children>
            <Label prefHeight="78.0" prefWidth="395.0" text="Student view">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="38.0" prefWidth="579.0" spacing="10.0">
         <children>
            <Label text="number">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="numberField" onAction="#searchByNumber" prefHeight="39.0" prefWidth="134.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Button mnemonicParsing="false" onAction="#searchByNumber" text="Search by number">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="156.0" prefHeight="38.0" prefWidth="579.0" spacing="10.0">
         <children>
            <Label prefHeight="27.0" prefWidth="63.0" text="name">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="nameField" onAction="#searchByName" prefHeight="39.0" prefWidth="134.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Button mnemonicParsing="false" onAction="#searchByName" text="Search by name">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
       <HBox alignment="CENTER_LEFT" prefHeight="38.0" prefWidth="579.0" spacing="10.0">
           <children>
               <Label prefHeight="27.0" prefWidth="63.0">
                   <font>
                       <Font size="18.0" />
                   </font>
               </Label>
               <Button mnemonicParsing="false" onAction="#createStudent" prefHeight="39.0" prefWidth="132.0" text="Create new">
                   <font>
                       <Font size="18.0" />
                   </font>
               </Button>
           </children>
           <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
           </VBox.margin>
       </HBox>
       <HBox alignment="CENTER_LEFT" prefHeight="120.0" prefWidth="460.0" spacing="10.0">
           <children>
               <Label fx:id="resultLabel" prefHeight="120.0" prefWidth="460.0" text="result" wrapText="true">
                   <font>
                       <Font size="18.0" />
                   </font>
               </Label>
            </children>
           <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
           </VBox.margin>
       </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="140.0" prefHeight="80.0" prefWidth="460.0" spacing="10.0">
         <children>
            <Label fx:id="messageLabel" prefHeight="78.0" prefWidth="395.0" text="message">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>

   </children>
</VBox>
